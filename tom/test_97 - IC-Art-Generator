int Grid_Size = 100;
int Grid_Dense = 10;
Chip chip;

void setup()
{
  size(800, 800);
  background(255);
  noSmooth();
  
  chip = new Chip(100, 100, Grid_Dense*5, Grid_Dense*4);
}

void draw()
{
  background(255);
  
  drawGrid(Grid_Size, Grid_Dense);
  
  chip.show();
}

void drawGrid(int GridSize, int GridDense)
{
  for(int i = 0; i < GridSize; i++)
  {
    for(int j = 0; j < GridSize; j++)
    {
      point(i*GridDense, j*GridDense);
    }
  }
}
